#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1 2001/04/22 06:25:19 jlam Exp $

PKGNAME=$1
STAGE=$2

RM="@RM@"

BSD_PERL_MK=${PKG_PREFIX}/share/mk/bsd.perl.mk

case ${STAGE} in
DEINSTALL)
	# Remove generated bsd.perl.mk
	#
	${RM} -f ${BSD_PERL_MK}
	;;

POST-DEINSTALL)
	;;

*)
	echo "Unexpected argument: ${STAGE}"
	exit 1
	;;
esac
exit 0
