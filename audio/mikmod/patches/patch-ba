+#!/bin/sh -e
+cd playercode
+gmake -f makefile.netbsd
+cd ..
+cd mmio
+gmake -f makefile.netbsd
+cd ..
+cd usercode
+gmake -f makefile.netbsd
+cp mikmod ../..
+cd ../..
+strip mikmod
+echo All done.
