#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1 2000/10/13 23:36:39 jlam Exp $

PKGNAME=$1
STAGE=$2

case ${STAGE} in
DEINSTALL)
	;;
POST-DEINSTALL)
	COMPAT_LIBDIR=@COMPAT_LIBDIR@

	rmdir -p ${PKG_PREFIX}/${COMPAT_LIBDIR} 2>/dev/null || true
	;;
*)
	echo "Unexpected argument: ${STAGE}"
	exit 1
	;;
esac
exit 0
