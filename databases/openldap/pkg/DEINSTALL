#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1 2000/08/14 16:35:32 wiz Exp $
#

case $2 in
        DEINSTALL) cat << EOF
===========================================================================
If you don't want to use openldap anymore, remove @PREFIX@/etc/openldap.
===========================================================================

EOF
        ;;
esac
