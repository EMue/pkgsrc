#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.2 2000/12/11 19:19:42 jlam Exp $
#

case $2 in
        DEINSTALL) cat << EOF
===========================================================================
If you don't want to use openldap anymore, remove ${PKG_PREFIX}/etc/openldap.
===========================================================================

EOF
        ;;
esac
