+#include <unistd.h>
+#include <stdlib.h>
+
+int
+main(argc, argv)
+	int argc;
+	char **argv;
+{
+	setenv("TCLLIBPATH", "@prefix@/lib/tkined@TKI_VERSION@ @prefix@/lib/tnm@TNM_VERSION@", 1);
+	execv("@prefix@/bin/tkined@TKI_VERSION@", argv);
+	exit(1);
+}
