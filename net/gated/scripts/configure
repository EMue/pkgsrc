#!/bin/sh
#
# $NetBSD: configure,v 1.2 1998/08/07 11:10:40 agc Exp $
#
# Configure gated
#
cd $WRKSRC

for file in Makefile src/Makefile src/util/Makefile.template ; do
	mv $file $file.orig
	sed -e 's/^true:;$/true:/' -e 's/^[ 	]*$//' $file.orig >$file
done

cd src

obj=obj.`util/archtype`
mkdir -p $obj

cp configs/netbsd $obj/Config
rm -f Config
