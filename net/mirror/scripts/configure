#!/bin/sh
#
# $NetBSD: configure,v 1.5 2000/08/27 02:46:05 jlam Exp $
#
# Id: configure,v 1.1.1.1 1997/10/27 05:01:31 hubertf Exp
#
cd ${WRKSRC}
for i in do_unlinks.pl mirror.pl mm.pl pkgs_to_mmin.pl
do
    sed \
	-e s:!!PREFIX!!:$PREFIX:g \
	-e s:/usr/bin/perl:${PERL5}: \
	<$i >$i.BAK
    mv -f $i.BAK $i
done
