#! /bin/sh
#
# $NetBSD: INSTALL,v 1.5 2000/03/02 05:11:45 kim Exp $
#

case "$2" in
  ""|POST-INSTALL) cat <<EOF
=============================================================

Some files you might need to customize include the following:

    ${PKG_PREFIX}/etc/nmh/mhn.defaults
    ${PKG_PREFIX}/etc/nmh/mts.conf

=============================================================
EOF
  ;;
esac

exit 0
