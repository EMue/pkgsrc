 ;; Hairy regexp matching tar `--list --verbose' output
 (defvar tarfh-list-regexp (concat "([a-zA-Z-]+)\\s+(\\w+)/(\\w+)\\s+(\\d+)\\s+"
-				  "([0-9-]+\\s+[0-9:]+)\\s+([^\n]+)"))
+				  "([A-Za-z]+\\s+[0-9]+\\s+[0-9:]+\\s+[0-9]+)\\s+([^\n]+)"))
 
 ;; Map list file types to symbols
 (defvar tarfh-list-type-alist '((?- . file) (?d . directory)
