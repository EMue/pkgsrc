# $NetBSD: DEINSTALL,v 1.1 2000/05/27 09:53:38 wiz Exp $
#

if [ "$2" != "POST-DEINSTALL" ]; then
    exit 0
fi

if ! user del glunix
then
    echo Removing user \'glunix\' failed, please remove him manually.
fi
if ! group del glunix
then
    echo Removing group  \'glunix\' failed, please remove it manually.
fi
